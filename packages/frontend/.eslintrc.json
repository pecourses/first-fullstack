{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        //"plugin:jsx-a11y/recommended",
        "semistandard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
        //"jsx-a11y"
    ], 
    "rules": {
        "react/react-in-jsx-scope": "off"
        // "react/jsx-filename-extension": [
        //     1,
        //     {
        //       "extensions": [".js"]
        //     }
        //   ]
    }
}
